@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">

    
    
    <table class="table">
        <thead>
            <tr>
                <th scope="col">Nazwa</th>
                <th scope="col">Kalorie/100g</th>
            </tr>
        </thead>
        
        <tbody>
            @foreach (var food in Model)
            {
                <tr>
                    <th scope="row">@food.name</th>
                    <td>@food.calories kcal</td>
                </tr>
            }
        </tbody>
        </table>
 
</div>

